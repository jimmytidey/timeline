<tr id="user_chart_summary_<%= chart.id %>">
  <td><%= link_to chart.user_id, user_url(chart.user_id) %> </td>
  <td><%= chart.id %> </td>
  <td><%= chart.title %> </td>
  <td>(<%= chart.start_year %> to <%= chart.end_year %>)</td>
  <td class="hits">
    
  <%= form_for chart, :url => "admins/#{chart.id}" do |f| %>
    <%= f.text_field :hits, :id => "hits_#{chart.id}" %>
    <%= f.submit "Save", :id => "submit_#{chart.id}" %>
  <% end %>

  </td>
  <td><%= chart.private %> </td>
  <td><%= chart.created_at.strftime("%d %b %y | %H:%M") %> </td>
  <td><%= chart.updated_at.strftime("%d %b %y | %H:%M") %></td>
  <td><%= chart.num_events %></td>
  <td class="action"><%= link_to 'View', timeline_chart_url(chart) %></td>
  <td class="action"><%= link_to 'Edit', edit_timeline_chart_url(chart)  %></td>
  <td class="action"><%= button_to_function "Delete", "if (confirm('Are you sure?')) deleteTimeline(#{chart.id})", :class => :delete_timeline_link %></td>
</tr>
