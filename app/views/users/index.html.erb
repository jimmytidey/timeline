<h3 id="user_list_heading">Listing users:</h3>
<table>
  <tr>
    <th>Name</th>
    <th>Identifier</th>
    <th>Created</th>
  </tr>
  <% @users.each do |user| %>
    <tr>
      <td><%= user.name %></td>
      <td><%= user.identifier %></td>
      <td><%= user.created_at %></td>
    </tr>
  <% end %>
</table>
